{
  "name": "laravel-mix-manifest-paths",
  "version": "2.0.1",
  "description": "A laravel-mix extension that grants access to the generated manifest's paths in static assets.",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "rimraf tests/coverage && jest",
    "test:coverage": "rimraf tests/coverage && jest --coverage",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.js --max-warnings 3",
    "lint:fix": "eslint . --ignore-path .gitignore --ext .ts,.js --fix",
    "build": "rimraf dist && tsc -p src",
    "clean": "rimraf dist tests/coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recursive-beast/laravel-mix-manifest-paths.git"
  },
  "keywords": [
    "laravel",
    "mix",
    "laravel-mix",
    "manifest",
    "url",
    "public",
    "webpack",
    "extension",
    "plugin"
  ],
  "author": {
    "name": "Soufiane Yakoubi",
    "email": "splentercell.1997@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/recursive-beast/laravel-mix-manifest-paths/issues"
  },
  "homepage": "https://github.com/recursive-beast/laravel-mix-manifest-paths#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/glob-parent": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/url-join": "^4.0.3",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "laravel-mix": "^6.0.49",
    "memfs": "^4.6.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.2",
    "string-argv": "^0.3.2",
    "ts-jest": "^29.1.2",
    "typescript": "5.5"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "dependency-graph": "^1.0.0",
    "glob": "^10.3.10",
    "glob-parent": "^6.0.2",
    "lodash": "^4.17.21",
    "tslib": "^2.6.2",
    "url-join": "^4.0.1"
  },
  "peerDependencies": {
    "laravel-mix": "6.x.x"
  }
}
