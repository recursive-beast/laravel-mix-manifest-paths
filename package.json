{
  "name": "laravel-mix-manifest-paths",
  "version": "1.1.2",
  "description": "A laravel-mix extension that grants access to the generated manifest's paths in static assets.",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "rimraf tests/coverage && jest",
    "test:coverage": "rimraf tests/coverage && jest --coverage",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.js --max-warnings 3",
    "lint:fix": "eslint . --ignore-path .gitignore --ext .ts,.js --fix",
    "build": "rimraf dist && tsc -p src",
    "clean": "rimraf dist tests/coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recursive-beast/laravel-mix-manifest-paths.git"
  },
  "keywords": [
    "laravel",
    "mix",
    "laravel-mix",
    "manifest",
    "url",
    "public",
    "webpack",
    "extension",
    "plugin"
  ],
  "author": {
    "name": "Soufiane Yakoubi",
    "email": "splentercell.1997@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/recursive-beast/laravel-mix-manifest-paths/issues"
  },
  "homepage": "https://github.com/recursive-beast/laravel-mix-manifest-paths#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/glob": "^7.2.0",
    "@types/glob-parent": "^5.1.1",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^17.0.18",
    "@types/url-join": "^4.0.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "codecov": "^3.8.3",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "laravel-mix": "^6.0.43",
    "mock-fs": "^5.1.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "string-argv": "^0.3.1",
    "ts-jest": "^27.1.3",
    "typescript": "4.5"
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.3",
    "dependency-graph": "^0.11.0",
    "glob": "^7.2.0",
    "glob-parent": "^6.0.2",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1",
    "url-join": "^4.0.1"
  },
  "peerDependencies": {
    "laravel-mix": "6.x.x"
  }
}
